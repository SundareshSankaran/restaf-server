{
  "name": "restaf-server",
  "version": "5.1.0",
  "description": "Custom app server for Viya apps",
  "author": "Deva Kumaraswamy <deva.kumar@sas.com>",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "keywords": [
    "restaf",
    "Viya",
    "SAS",
    "server",
    "RESTAPI"
  ],
  "engines": {
    "node": ">=8.9.0"
  },
  "bin": {
    "restaf-server": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sassoftware/restaf-server.git"
  },
  "bugs": {
    "url": "https://github.com/sassoftware/restaf-server/issues"
  },
  "scripts": {
    "start": "node app",
    "test": "node app myapp.env",
    "app": "node cli --env=myapp.env --appenv=appenv.js",
    "proxy": "node app proxy.env",
    "webpack": "webpack --config webpack.config.js",
    "build": "babel src --out-dir lib",
    "lint": "./node_modules/.bin/eslint --fix src/*.js"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-polyfill": "^6.26.0",
    "bell": "^9.4.0",
    "boom": "^7.2.2",
    "debug": "^4.1.0",
    "hapi": "^17.8.1",
    "hapi-auth-cookie": "^9.0.0",
    "inert": "^5.1.2",
    "minimist": "^1.2.0",
    "qs": "^6.5.2",
    "request": "^2.88.0",
    "request-debug": "^0.2.0",
    "shelljs": "^0.8.3",
    "tcp-ping": "^0.1.1",
    "uuid": "^3.3.2",
    "wreck": "^14.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^5.9.0",
    "prettyjson": "^1.2.1",
    "webpack-cli": "^3.1.2",
    "yargs": "^12.0.5"
  }
